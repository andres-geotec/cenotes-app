<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/cenotesSavedFragment">
    <fragment
        android:id="@+id/cenotesSavedFragment"
        android:name="com.geotec.cenotesapp.ui.cenote.CenotesSavedFragment"
        android:label="fragment_cenotes_saved"
        tools:layout="@layout/fragment_cenotes_saved" >
        <action
            android:id="@+id/action_cenotesSavedFragment_to_editorCenoteFragment"
            app:destination="@id/editorCenoteFragment" />
        <action
            android:id="@+id/action_cenotesSavedFragment_to_cenoteSectionsFragment"
            app:destination="@id/cenoteSectionsFragment" />
    </fragment>
    <fragment
        android:id="@+id/editorCenoteFragment"
        android:name="com.geotec.cenotesapp.ui.cenote.EditorCenoteFragment"
        android:label="fragment_editor_cenote"
        tools:layout="@layout/fragment_editor_cenote" />
    <fragment
        android:id="@+id/cenoteSectionsFragment"
        android:name="com.geotec.cenotesapp.ui.cenote.sections.CenoteSectionsFragment"
        android:label="fragment_cenote_sections"
        tools:layout="@layout/fragment_cenote_sections" >
        <action
            android:id="@+id/action_cenoteSectionsFragment_to_cenoteGeneralSecFragment"
            app:destination="@id/cenoteGeneralSecFragment" />
        <action
            android:id="@+id/action_cenoteSectionsFragment_to_cenoteClasifiSecFragment"
            app:destination="@id/cenoteClasifiSecFragment" />
    </fragment>
    <fragment
        android:id="@+id/cenoteGeneralSecFragment"
        android:name="com.geotec.cenotesapp.ui.cenote.sections.CenoteGeneralSecFragment"
        android:label="fragment_cenote_general_sec"
        tools:layout="@layout/fragment_cenote_general_sec" />
    <fragment
        android:id="@+id/cenoteClasifiSecFragment"
        android:name="com.geotec.cenotesapp.ui.cenote.sections.CenoteClasifiSecFragment"
        android:label="fragment_cenote_clasifi_sec"
        tools:layout="@layout/fragment_cenote_clasifi_sec" />
</navigation>